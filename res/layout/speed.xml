<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txt1"
        android:layout_below="@+id/txt1"
        android:layout_marginTop="25dp"
        android:text="@string/username"
        android:textColor="@color/White"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/usern_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/fst_name_speed"
        android:layout_alignRight="@+id/fst_name_speed"
        android:layout_alignTop="@+id/textView2"
        android:background="@color/White"
        android:textColor="@color/Black"
        android:textSize="20sp"
        android:width="120dp" />

     <TextView
         android:id="@+id/total_dist_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBottom="@+id/attempt_view"
         android:layout_alignRight="@+id/usern_speed"
         android:layout_alignTop="@+id/attempt_view"
         android:layout_toRightOf="@+id/attempt_view"
         android:background="@color/White"
         android:textColor="@color/Black"
         android:textSize="20sp" />

     <TextView
         android:id="@+id/attempt_view"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/start_speed"
         android:layout_alignParentLeft="true"
         android:layout_marginBottom="20dp"
         android:layout_marginLeft="16dp"
         android:paddingLeft="10dp"
         android:paddingRight="10dp"
         android:text="@string/tot_distance"
         android:textColor="@color/White"
         android:textSize="20sp" />

     <TextView
         android:id="@+id/textView6"
         android:layout_width="match_parent"
         android:layout_height="5dp"
         android:layout_alignParentLeft="true"
         android:layout_below="@+id/textView2"
         android:layout_marginTop="15dp"
         android:background="@color/teal" />

     <Button
         android:id="@+id/start_speed"
         style="?android:attr/buttonStyleSmall"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentBottom="true"
         android:layout_centerHorizontal="true"
         android:layout_marginBottom="45dp"
         android:background="@color/Teal"
         android:height="30dp"
         android:text="@string/start"
         android:width="70dp" />

     <TextView
         android:id="@+id/fst_name_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBaseline="@+id/txt1"
         android:layout_alignBottom="@+id/txt1"
         android:layout_marginLeft="19dp"
         android:layout_toRightOf="@+id/txt1"
         android:background="@color/white"
         android:textColor="@color/Black"
         android:textSize="20sp"
         android:width="120dp" />

     <TextView
         android:id="@+id/textView4"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/txt1"
         android:layout_below="@+id/textView3"
         android:layout_marginBottom="15dp"
         android:text="@string/at3"
         android:textColor="@color/White"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/textView1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView6"
         android:layout_marginTop="15dp"
         android:layout_toLeftOf="@+id/fst_name_speed"
         android:text="@string/at1"
         android:textColor="@color/White"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/textView3"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView1"
         android:layout_marginTop="15dp"
         android:layout_marginBottom="15dp"
         android:layout_toLeftOf="@+id/fst_name_speed"
         android:text="@string/at2"
         android:textColor="@color/White"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/textView5"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView4"
         android:layout_toLeftOf="@+id/fst_name_speed"
         android:text="@string/at4"
         android:textColor="@color/White"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/textView7"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/textView5"
         android:layout_below="@+id/textView5"
         android:layout_marginTop="16dp"
         android:text="@string/at5"
         android:textColor="@color/white"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/textView8"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView7"
         android:layout_marginTop="14dp"
         android:layout_toLeftOf="@+id/fst_name_speed"
         android:text="@string/at6"
         android:textColor="@color/white"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att1_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/textView3"
         android:layout_alignLeft="@+id/usern_speed"
         android:layout_alignRight="@+id/usern_speed"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att2_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBottom="@+id/textView3"
         android:layout_alignLeft="@+id/att1_speed"
         android:layout_alignRight="@+id/att1_speed"
         android:layout_alignTop="@+id/textView3"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att3_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBottom="@+id/textView4"
         android:layout_alignLeft="@+id/att2_speed"
         android:layout_alignRight="@+id/att2_speed"
         android:layout_alignTop="@+id/textView4"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att4_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/textView7"
         android:layout_alignLeft="@+id/att3_speed"
         android:layout_alignRight="@+id/att3_speed"
         android:layout_below="@+id/textView4"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att5_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/textView8"
         android:layout_alignLeft="@+id/att4_speed"
         android:layout_alignRight="@+id/att4_speed"
         android:layout_alignTop="@+id/textView7"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/att6_speed"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBottom="@+id/textView8"
         android:layout_alignLeft="@+id/att5_speed"
         android:layout_alignRight="@+id/att5_speed"
         android:layout_alignTop="@+id/textView8"
         android:background="@color/white"
         android:paddingLeft="10dp"
         android:textColor="@color/black"
         android:textSize="15sp" />

     <TextView
         android:id="@+id/txt1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/attempt_view"
         android:layout_alignParentTop="true"
         android:layout_marginLeft="16dp"
         android:layout_marginTop="27dp"
         android:paddingRight="10dp"
         android:text="@string/fname"
         android:textColor="@color/white"
         android:textSize="20sp" />

     <TextView
         android:id="@+id/textView9"
         android:layout_width="match_parent"
         android:layout_height="5dp"
         android:layout_above="@+id/total_dist_speed"
         android:layout_alignParentLeft="true"
         android:layout_marginBottom="17dp"
         android:background="@color/teal" />

</RelativeLayout>
